{
  "name": "dreammaker-crypto-signal-trader",
  "version": "1.0.0",
  "description": "Advanced cryptocurrency signal analysis and trading platform with AI-powered insights",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "vite",
    "dev:server": "tsx watch --clear-screen=false src/server.ts",
    "dev:kill": "node scripts/kill-port.cjs",
    "dev:win": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/win/dev.ps1",
    "build": "tsc && vite build",
    "build:client": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "start": "node dist/server.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:api-health": "tsx scripts/test-api-health.ts",
    "test:optional:public": "tsx scripts/test-optional-public.ts",
    "test:optional:news": "tsx scripts/test-optional-news.ts",
    "test:optional:market": "tsx scripts/test-optional-market.ts",
    "test:optional:onchain": "tsx scripts/test-optional-onchain.ts",
    "test:optional:all": "npm run test:optional:public && npm run test:optional:news && npm run test:optional:market && npm run test:optional:onchain",
    "test:pipeline": "tsx scripts/test-full-pipeline.ts",
    "test:hf": "tsx scripts/test-hf-integration.ts",
    "test:hf-engine": "tsx scripts/test-hf-engine-integration.ts",
    "test:proxy": "tsx scripts/test-proxy.ts",
    "test:smoke": "node scripts/smoke_local.mjs",
    "test:free-resources": "node free_resources_selftest.mjs",
    "test:free-resources:8001": "set API_BASE=http://localhost:8001/api && node free_resources_selftest.mjs",
    "test:free-resources:win": "powershell -NoProfile -ExecutionPolicy Bypass -File test_free_endpoints.ps1 -ApiBase http://localhost:8001/api",
    "test:free-resources:sh": "bash test_free_endpoints.sh http://localhost:8001/api",
    "test:api": "tsx src/testing/cli.ts all",
    "test:api:market": "tsx src/testing/cli.ts market",
    "test:api:integration": "tsx src/testing/cli.ts integration",
    "test:api:performance": "tsx src/testing/cli.ts performance",
    "test:api:security": "tsx src/testing/cli.ts security",
    "test:api:concurrent": "tsx src/testing/cli.ts concurrent 20",
    "test:api:load": "tsx src/testing/cli.ts load 10 10",
    "test:api:report": "tsx src/testing/cli.ts all --output ./reports --format markdown",
    "test:api:smart": "tsx scripts/test-api-smart.ts",
    "e2e:smoke": "playwright test e2e/smoke.spec.ts --project=chromium",
    "e2e:smoke:win": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/run-e2e-tests.ps1",
    "e2e:ui": "playwright test --ui",
    "e2e:report": "playwright show-report",
    "test:netcheck": "curl -s http://localhost:8000/api/system/diagnostics/netcheck | jq || curl -s http://localhost:8000/api/system/diagnostics/netcheck",
    "verify:ops": "curl -sf http://localhost:8000/api/health && curl -s http://localhost:8000/api/system/diagnostics/netcheck | jq && curl -s http://localhost:8000/api/system/metrics | jq",
    "safe:merge-json": "node scripts/safe-merge-json.mjs",
    "safe:add-env": "bash scripts/safe-add-env.sh",
    "safe:update-env": "bash scripts/safe-add-env.sh .env.example",
    "preview": "vite preview --host 0.0.0.0 --port 5173",
    "preview:win": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/win/preview.ps1",
    "typecheck": "tsc --noEmit",
    "ci:build": "npm run build:client && npm run build:server",
    "ci:typecheck": "npm run typecheck || tsc -p tsconfig.json --noEmit",
    "ci:lint": "npm run lint -- --max-warnings=0 || true",
    "ci:test": "npm test -- --run --reporter=dot",
    "smoke": "node scripts/smoke.mjs",
    "self:heal": "bash scripts/self-heal.sh",
    "oneclick:15m": "bash scripts/oneclick-15m.sh",
    "test:gh-pages": "bash scripts/test-gh-pages-build.sh",
    "release:patch": "node scripts/release.mjs patch",
    "release:minor": "node scripts/release.mjs minor",
    "release:major": "node scripts/release.mjs major",
    "release:rc": "node scripts/release.mjs rc",
    "release:set": "node scripts/release.mjs set",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "better-sqlite3": "^12.4.1",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "fast-xml-parser": "^5.3.1",
    "global-agent": "^3.0.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "isomorphic-ws": "^5.0.0",
    "lucide-react": "^0.294.0",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "prom-client": "^15.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.1.2",
    "@types/better-sqlite3": "^7.6.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.25",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20.19.25",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^4.0.8",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "esbuild": "^0.27.0",
    "eslint": "^9.0.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.0.0",
    "jsdom": "^23.0.1",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.0.0",
    "vite": "^7.2.2",
    "vitest": "^4.0.8",
    "whatwg-fetch": "^3.6.20"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nimazasinich/DreammakerCryptoSignalAndTrader.git"
  },
  "keywords": [
    "cryptocurrency",
    "trading",
    "signals",
    "technical-analysis",
    "ai",
    "futures",
    "kucoin",
    "binance"
  ],
  "author": "",
  "license": "Unlicense"
}
